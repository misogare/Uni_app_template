<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/base_background"
    tools:context=".BlankFragment2">

    <TextView
        android:id="@+id/meow"
        android:layout_width="361dp"
        android:layout_height="53dp"
        android:layout_marginStart="4dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Turn on Notifications "
        android:textSize="26dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.451"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.112" />

    <TextView
        android:id="@+id/wew"
        android:layout_width="355dp"
        android:layout_height="88dp"

        android:layout_marginStart="4dp"
        android:layout_marginTop="22dp"

        android:text="aLorem ipsum dolor sit amet,
         consectetur adipiscing elit. Sit enim, ac amet ultrices."
        android:textColor="#575757"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@id/meow"
        app:layout_constraintTop_toBottomOf="@id/meow" />

    <ImageView
        android:id="@+id/lal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:src="@drawable/svgte"
        app:layout_constraintEnd_toEndOf="@id/wew"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="@id/wew"
        app:layout_constraintTop_toBottomOf="@id/wew"></ImageView>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/omg"
        android:layout_width="327dp"
        android:layout_height="56dp"
        android:layout_marginTop="26dp"
        android:background="@drawable/buttonsuperior"
        android:fontFamily="@font/poppins"
        android:text="Turn On"
        android:textColor="@color/white"
        android:textSize="17dp"
        app:layout_constraintEnd_toEndOf="@id/wew"
        app:layout_constraintHorizontal_bias="0.821"
        app:layout_constraintStart_toStartOf="@id/wew"
        app:layout_constraintTop_toBottomOf="@id/lal"></androidx.appcompat.widget.AppCompatButton>

    <androidx.appcompat.widget.AppCompatButton
        android:layout_width="327dp"
        android:layout_height="56dp"
        android:layout_marginTop="26dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/poppins"
        android:text="Remind me later"
        android:textColor="#03A9F4"
        android:textSize="17dp"
        app:layout_constraintEnd_toEndOf="@id/omg"
        app:layout_constraintHorizontal_bias="0.821"
        app:layout_constraintStart_toStartOf="@id/omg"
        app:layout_constraintTop_toBottomOf="@id/omg"></androidx.appcompat.widget.AppCompatButton>

</androidx.constraintlayout.widget.ConstraintLayout>
